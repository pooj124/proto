pipeline {
    agent any
    stages {
        stage('Build') {
            steps {
                // Get some code from a GitHub repository
                git url: 'https://github.com/pooj124/proto.git', branch: 'main'
                // Change file permisson
                sh "chmod +x -R ./jenkins *.sh"
                // Run shell script
                sh "./make.sh"
            }
        }
    }
}
